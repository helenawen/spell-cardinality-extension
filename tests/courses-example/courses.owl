<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>

<rdf:RDF xmlns="http://example.com/university#"
     xml:base="http://example.com/university"
     xmlns:rdf="&rdf;"
     xmlns:owl="&owl;"
     xmlns:xsd="&xsd;"
     xmlns:rdfs="&rdfs;">

    <owl:Ontology rdf:about="http://example.com/university"/>

    <!-- Object Property -->
    <owl:ObjectProperty rdf:about="http://example.com/university#takesCourse">
        <rdfs:domain rdf:resource="http://example.com/university#Person"/>
        <rdfs:range rdf:resource="http://example.com/university#Course"/>
    </owl:ObjectProperty>

    <!-- Classes -->
    <owl:Class rdf:about="http://example.com/university#Person"/>

    <owl:Class rdf:about="http://example.com/university#Course"/>

    <owl:Class rdf:about="http://example.com/university#AdvancedCourse">
        <rdfs:subClassOf rdf:resource="http://example.com/university#Course"/>
    </owl:Class>

    <!-- Courses -->
    <owl:NamedIndividual rdf:about="http://example.com/university#AI101">
        <rdf:type rdf:resource="http://example.com/university#AdvancedCourse"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.com/university#ML202">
        <rdf:type rdf:resource="http://example.com/university#AdvancedCourse"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.com/university#NLP303">
        <rdf:type rdf:resource="http://example.com/university#AdvancedCourse"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.com/university#DB101">
        <rdf:type rdf:resource="http://example.com/university#AdvancedCourse"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.com/university#IntroLogic">
        <rdf:type rdf:resource="http://example.com/university#Course"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.com/university#Philosophy101">
        <rdf:type rdf:resource="http://example.com/university#Course"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.com/university#ArtHistory">
        <rdf:type rdf:resource="http://example.com/university#Course"/>
    </owl:NamedIndividual>

    <!-- Individuals: Helena, Linus, Jakob -->

    <!-- Helena -->
    <owl:NamedIndividual rdf:about="http://example.com/university#Helena">
        <rdf:type rdf:resource="http://example.com/university#Person"/>
        <takesCourse rdf:resource="http://example.com/university#AI101"/>
        <takesCourse rdf:resource="http://example.com/university#ML202"/>
        <takesCourse rdf:resource="http://example.com/university#NLP303"/>
    </owl:NamedIndividual>

    <!-- Linus-->
    <owl:NamedIndividual rdf:about="http://example.com/university#Linus">
        <rdf:type rdf:resource="http://example.com/university#Person"/>
        <takesCourse rdf:resource="http://example.com/university#DB101"/>
        <takesCourse rdf:resource="http://example.com/university#AI101"/>
        <takesCourse rdf:resource="http://example.com/university#ML202"/>
        <takesCourse rdf:resource="http://example.com/university#ArtHistory"/>
    </owl:NamedIndividual>

    <!-- Jakob-->
    <owl:NamedIndividual rdf:about="http://example.com/university#Jakob">
        <rdf:type rdf:resource="http://example.com/university#Person"/>
        <takesCourse rdf:resource="http://example.com/university#IntroLogic"/>
        <takesCourse rdf:resource="http://example.com/university#Philosophy101"/>
        <takesCourse rdf:resource="http://example.com/university#ArtHistory"/>
        <takesCourse rdf:resource="http://example.com/university#ML202"/>
    </owl:NamedIndividual>

</rdf:RDF>
