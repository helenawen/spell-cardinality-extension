<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>

<rdf:RDF xmlns="http://example.com/family#"
     xml:base="http://example.com/family"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://example.com/family"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://example.com/family#hasChild">
        <rdfs:domain rdf:resource="http://example.com/family#person"/>
        <rdfs:range rdf:resource="http://example.com/family#person"/>
    </owl:ObjectProperty>

    <!-- Classes -->
    <owl:Class rdf:about="http://example.com/family#person"/>
    <owl:Class rdf:about="http://example.com/family#female">
        <rdfs:subClassOf rdf:resource="http://example.com/family#person"/>
    </owl:Class>
    <owl:Class rdf:about="http://example.com/family#male">
        <owl:equivalentClass>
            <owl:Class>
                <owl:complementOf rdf:resource="http://example.com/family#female"/>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://example.com/family#person"/>
    </owl:Class>

    <!-- Individuals -->

    <!-- Hemma (f) -->
    <owl:NamedIndividual rdf:about="http://example.com/family#hemma">
        <rdf:type rdf:resource="http://example.com/family#person"/>
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <hasChild rdf:resource="http://example.com/family#helena"/>
        <hasChild rdf:resource="http://example.com/family#petz"/>
        <hasChild rdf:resource="http://example.com/family#sophia"/>
        <hasChild rdf:resource="http://example.com/family#rita"/>
    </owl:NamedIndividual>

    <!-- Peter (m) -->
    <owl:NamedIndividual rdf:about="http://example.com/family#peter">
        <rdf:type rdf:resource="http://example.com/family#person"/>
        <rdf:type rdf:resource="http://example.com/family#male"/>
        <hasChild rdf:resource="http://example.com/family#helena"/>
        <hasChild rdf:resource="http://example.com/family#petz"/>
        <hasChild rdf:resource="http://example.com/family#sophia"/>
        <hasChild rdf:resource="http://example.com/family#rita"/>
    </owl:NamedIndividual>

    <!-- Children of Hemma & Peter -->
    <owl:NamedIndividual rdf:about="http://example.com/family#helena">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://example.com/family#petz">
        <rdf:type rdf:resource="http://example.com/family#male"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://example.com/family#sophia">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://example.com/family#rita">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>

    <!-- Eva (f) -->
    <owl:NamedIndividual rdf:about="http://example.com/family#eva">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
        <hasChild rdf:resource="http://example.com/family#hannes"/>
    </owl:NamedIndividual>

    <!-- Walter (m) -->
    <owl:NamedIndividual rdf:about="http://example.com/family#walter">
        <rdf:type rdf:resource="http://example.com/family#male"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
        <hasChild rdf:resource="http://example.com/family#hannes"/>
        <hasChild rdf:resource="http://example.com/family#lisbeth"/>
    </owl:NamedIndividual>

    <!-- Children of Eva & Walter -->
    <owl:NamedIndividual rdf:about="http://example.com/family#hannes">
        <rdf:type rdf:resource="http://example.com/family#male"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://example.com/family#lisbeth">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>

       <!-- Maria (f) -->
    <owl:NamedIndividual rdf:about="http://example.com/family#maria">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
        <hasChild rdf:resource="http://example.com/family#sara"/>
        <hasChild rdf:resource="http://example.com/family#anna"/>
        <hasChild rdf:resource="http://example.com/family#paul"/>
    </owl:NamedIndividual>

    <!-- Children of Maria -->
    <owl:NamedIndividual rdf:about="http://example.com/family#sara">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://example.com/family#anna">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://example.com/family#paul">
        <rdf:type rdf:resource="http://example.com/family#male"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>

        <!-- Patricia (f) -->
    <owl:NamedIndividual rdf:about="http://example.com/family#patricia">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
        <hasChild rdf:resource="http://example.com/family#vannessa"/>
        <hasChild rdf:resource="http://example.com/family#janine"/>
    </owl:NamedIndividual>

    <!-- Children of Patricia -->
    <owl:NamedIndividual rdf:about="http://example.com/family#vannessa">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="http://example.com/family#janine">
        <rdf:type rdf:resource="http://example.com/family#female"/>
        <rdf:type rdf:resource="http://example.com/family#person"/>
    </owl:NamedIndividual>

