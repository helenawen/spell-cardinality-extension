<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns="http://example.org/hobbies#"
         xml:base="http://example.org/hobbies">

    <!-- Ontology header -->
    <owl:Ontology rdf:about="http://example.org/hobbies">
        <rdfs:comment>Ontology describing individuals and their activities (sports and crafts).</rdfs:comment>
    </owl:Ontology>

    <!-- Classes -->
    <owl:Class rdf:about="#Person"/>
    <owl:Class rdf:about="#Sport"/>
    <owl:Class rdf:about="#Craft"/>

    <!-- Object properties -->
    <owl:ObjectProperty rdf:about="#doesSport">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Sport"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#doesCraft">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Craft"/>
    </owl:ObjectProperty>

    <owl:NamedIndividual rdf:about="#Football">
        <rdf:type rdf:resource="#Sport"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Running">
        <rdf:type rdf:resource="#Sport"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Boulders">
        <rdf:type rdf:resource="#Sport"/>
    </owl:NamedIndividual>

    <!-- Crafts -->
    <owl:NamedIndividual rdf:about="#Drawing">
        <rdf:type rdf:resource="#Craft"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Crochet">
        <rdf:type rdf:resource="#Craft"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Painting">
        <rdf:type rdf:resource="#Craft"/>
    </owl:NamedIndividual>

    <!-- People -->
    <owl:NamedIndividual rdf:about="#Linus">
        <rdf:type rdf:resource="#Person"/>
        <doesSport rdf:resource="#Boulders"/>
        <doesSport rdf:resource="#Football"/>
        <doesSport rdf:resource="#Running"/>
        <doesCraft rdf:resource="#Drawing"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Helena">
        <rdf:type rdf:resource="#Person"/>
        <doesCraft rdf:resource="#Crochet"/>
        <doesCraft rdf:resource="#Painting"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Jakob">
        <rdf:type rdf:resource="#Person"/>
        <doesSport rdf:resource="#Boulders"/>
    </owl:NamedIndividual>

</rdf:RDF>
